    "pre_install": [
        "Remove-Item \"$dir\\`$TEMP\" -Force -Recurse",
        "Remove-Item \"$dir\\`$PLUGINSDIR\" -Force -Recurse",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "Write-Host (\"`nPersisting $env:AppData\\Roaming\\aDrive ... \") -NoNewline",
        "New-PersistDirectory \"$env:APPDATA\\aDrive\" \"$persist_dir\\aDrive\" -Migrate"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "Confirm-Action",
        "Stop-App",
        "Write-Host (\"`nDeleting $env:APPDATA\\aDrive ... \") -f DarkGray -NoNewline",
        "Remove-Junction \"$env:APPDATA\\aDrive\" -ErrorAction SilentlyContinue | Out-Null",
        "Write-Host (\"`nDeleting $env:APPDATA\\utForpc ... \") -f DarkGray -NoNewline",
        "Remove-Item \"$env:APPDATA\\utForpc\" -Force -Recurse -ErrorAction SilentlyContinue | Out-Null"
    ],
    "checkver": {
        "url": "https://api.github.com/repos/microsoft/winget-pkgs/commits?path=manifests/a/Alibaba/aDrive",
        "regex": "New version.*? ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://cfproxy.1122330.xyz/https://cdn.aliyundrive.net/downloads/apps/desktop/update/$version/win32/x64/aDrive-$version.exe#/dl.7z",
        "hash": {
